{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "akakabu.vercel.app"
        }
      ],
      "status": 308,
      "headers": {
        "Location": "https://www.akakabu-project.com/$1"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}