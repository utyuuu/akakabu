{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "akakabu.vercel.app"
        }
      ],
      "destination": "https://www.akakabu-project.com/:1",
      "permanent": true
    }
  ]
}